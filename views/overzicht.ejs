<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Minor Web Development enquete</title>
    <link rel="stylesheet" href="/styles/style.css">
</head>

<body>
    <header>
        <a href="/" id="back">< terug</a>
        <h1>Minor Web Development</h1>
    </header>
    <main>
        <h2>Lijst met ingevulde enquetes</h2>
        <br />
        <section id="results">
            <% data.forEach(function(item){ %>
                <a href="/detail/<%- item._id %>">
                    <article>
                        <p class="name"><strong>Naam student: <%- item.studentName %></strong></p>
                        <p class="studentID">ID student: <%- item.studentID %></p>
                    </article>
                </a>
                <br />
            <% }) %>
        </section>
    </main>
</body>

</html>